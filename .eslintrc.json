{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", "tab"],
        "arrow-spacing": ["error"],
        "max-len": ["error", {"code": 80}]
    }
}
